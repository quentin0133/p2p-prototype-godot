script/source = "extends ColorRect

signal submit_lobby;

@export var client_manager: ClientManager;

@onready var username: LineEdit = $Background/HBoxContainer3/UsernameTextEdit;
@onready var lobby_id: LineEdit = $Background/HBoxContainer2/IdLobbyTextEdit;

func _ready() -> void:
	visibility_changed.connect(reset_form);

func _exit_tree() -> void:
	if (visibility_changed.is_connected(reset_form)):
		visibility_changed.disconnect(reset_form);

func reset_form():
	if (!visible): return;
	username.text = \"\";
	lobby_id.text = \"\";

func submit():
	username.text = username.text.strip_edges();
	if (username.text == \"\"):
		# feedback error
		return;
	if (lobby_id.text == \"\"):
		# feedback error
		return;
	var player = Player.new(LobbyWebSocket.websocket_user_id, username.text);
	var response = await LobbyWebSocket.join_lobby_post(lobby_id.text, player);
	if response.error:
		PopUpManager.show_pop_up(\"Erreur\", \"Le lobby avec cette id n'a pas été trouvé\", \"Ok\");
	client_manager.join_game(player);
	submit_lobby.emit();
"

[sub_resource type="GDScript" id="GDScript_2edry"]
resource_name = "FormClientLFG"
script/source = "class_name FormClientLFG
extends Control

signal submit_lobby;

@export var client_manager: ClientManager;

@onready var username: LineEdit = $Background/HBoxContainer3/UsernameTextEdit;

var lobby_id: String;

func _ready() -> void:
	visibility_changed.connect(reset_form);

func _exit_tree() -> void:
	if (visibility_changed.is_connected(reset_form)):
		visibility_changed.disconnect(reset_form);

func reset_form():
	if (!visible): return;
	username.text = \"\";

func show_form(lobby_id: String):
	self.lobby_id = lobby_id;
	visible = true;

func submit():
	username.text = username.text.strip_edges();
	if (username.text == \"\"):
		# feedback error
		return;
	var player = Player.new(LobbyWebSocket.websocket_user_id, username.text);
	await LobbyWebSocket.join_lobby_post(lobby_id, player);
	client_manager.join_game(player);
	submit_lobby.emit();
"

